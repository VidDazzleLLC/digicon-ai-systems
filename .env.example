# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Database
DATABASE_URL=your_database_url

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_secret_here

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Base URL for redirects and webhooks
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Email Configuration (SMTP)
# Option 1: Use SMTP (nodemailer)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Option 2: Use Resend (already configured in project)
RESEND_API_KEY=re_your_resend_api_key

# Note: Email delivery will fallback to console.log if neither SMTP nor Resend is configured

